{
  "expo": {
    "name": "UrbanFix AI",
    "slug": "frontend",
    "version": "1.1.0",
    "scheme": "urbanfix",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.urbanfix.ai"
    },
    "android": {
      "package": "com.urbanfix.ai",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyApHoIvTWWPC84MhWG6vnE4kfT_J63bprU"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b51bc665-c34d-47f9-b72f-a7bed6b5d6d3"
      }
    }
  }
}
